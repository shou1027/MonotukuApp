<% provide(:title,"投稿編集") %>

<div class="post-container">
  <div class="post-contents">
    <form action="" autocomplete="off">
      <input id="post-title" type="text" placeholder="タイトル">
      <div class="post-main-image-container">
        <div class="post-main-image">
          <img id="post-image" src="./和順商事株式会社.png">
        </div>
      </div>
      <input id="post-file" type="file">
      <input id="post-image-select" type="button" value="画像を選択">
      <div class="post-text">
        <input id="post-tag" type="text" placeholder="タグ　例:電子回路 組み込み">
        <textarea id="hoge" placeholder="本文"></textarea>
      </div>
      <input type="submit">
    </form>
  </div>
</div>